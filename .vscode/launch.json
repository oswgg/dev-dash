{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug NestJS (CommonJS)",
        "runtimeArgs": [
          "--require",
          "ts-node/register",
          "--inspect-brk",
          "src/main.ts"
        ],
        "envFile": "${workspaceFolder}/.env",
        "cwd": "${workspaceFolder}",
        "skipFiles": ["<node_internals>/**", "node_modules/**"]
      }
    ]
  }
  